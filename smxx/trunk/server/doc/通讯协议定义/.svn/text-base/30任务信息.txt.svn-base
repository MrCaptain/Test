%------------------------------------------
%Protocol:30003 接受任务
%------------------------------------------  
c >> s:
  int:16 TaskId 任务id 
s >> c:
  int:16 Result 操作结果 100:成功  101：任务不存在 102：任务已接受 103：等级不足 104：任务已经完成不能再接
end
 
%------------------------------------------
%Protocol: 30004 完成任务并挑选奖励
%------------------------------------------  
c>>s:
   int:16 TaskId 任务id  
s>>c:
   int:16 Result 操作结果 100：成功 201：任务未完成 202：角色未接受改任务
end 

%------------------------------------------
%Protocol:30005 查询任务npc状态
%------------------------------------------   
c>>s:
   array {  NpcList 任务列表
   int:16 NpcId
   } 
s>>c:
   array { NpcList 任务列表
   int:16 NpcId 
   int:8  NpcState npc状态(0:无任何状态 1：有未完成任务2：有可接任务3：有完成任务)
   }
end 

%------------------------------------------
%Protocol:30006 获取指定长度任务列表
%------------------------------------------    
c>>s: 
   int:8 Len 列表长度 
s>>c:
   array {  TaskList 任务列表
   int:16 TaskId 任务id
   int:8  TaskState 任务状态
   int:8  TaskProcess 任务进度
   }
 end
 %------------------------------------------
%Protocol:30007 消耗元宝自动完成任务
%------------------------------------------    
c>>s: 
    int:16 TaskId 任务id
s>>c:
    int:16 Result 结果(100:操作成功 201:任务没有触发 207:未知错误 206:任务不支持元宝完成 205:不够钱)
 end 
%------------------------------------------
%Protocol:30008 获取npc任务信息
%------------------------------------------    
c>>s: 
    int:16 NpcId Npcid
s>>c:
   array {  TaskList 任务列表
   int:16 TaskId 任务id
   int:8  TaskState 任务状态 
   }
end
%------------------------------------------
%Protocol:30100 通知服务器玩家与某任务npc对话
%------------------------------------------    
c>>s: 
    int:16 NpcId Npcid
s>>c:
end
%------------------------------------------
%Protocol: 30501 服务端通知客户端任务的完成条件已满足
%------------------------------------------  
c>>s:
s>>c:
    array { TaskList 任务列表
    int:16 TaskId 任务id
    int:8 State 任务状态
    int:8 NowNum 现价段完成的数量
    }
end   
%------------------------------------------
%Protocol:30503 通知客户端服务器为玩家触发了自动触发任务(列表)
%------------------------------------------  
c>>s:
s>>c:
    array { TaskList 任务列表
    int:16 TaskId 任务id
    }
end 
%------------------------------------------
%Protocol:30505 通知客户端服务器为玩家自动触发了某个任务
%------------------------------------------  
c>>s:
s>>c:
	int:16 TaskId 任务id
end 

%------------------------------------------
%Protocol:30506 通知客户端服务器为玩家自动完成了某个任务
%------------------------------------------   
c>>s:
s>>c:
	int:16 TaskId 任务id
end

%------------------------------------------
%Protocol:30507  通知客户端日常任务重置
%------------------------------------------   
c>>s:
s>>c:
    array { TaskList 任务列表
    int:8 Type 任务类型
    }
end