    int:32 unix时间戳
    string 平台用户账号
    string ticket
s >> c:
   int:8 
        0 => 成功登陆，有角色
        1 => 成功登陆，无角色
        2 => 校验失败
        3 => 失败 - 离线时间还没超过5小时（防沉迷）
	int:32 	acid
	int:64 	角色ID
	int:8  	状态(玩家状态: 0正常、1禁止)
	int:8  	职业
	int:8  	性别
	int:8  	等级
	string 	角色名字

