################# 个人排行  ##################
协议号:22001                             
c >> s:       
	int:8   0等级排行, 1日增(经验)排行、2周增(经验)排行
s >> c:								%%压缩
	int:8	0失败, 1成功
	int:32	更新时间					
	int:16  本人排名	%%若超出3000, 排名为0
	int:8   本人等级
	int:32  本人经验
	int:8   排名状态  0未变, 1上升, 2下降
	array(
		int:16  排名
		int:32  角色ID
		string  角色名
		int:8   职业
		int:8   等级
		int:32  经验
		int:8   排名状态  0未变, 1上升, 2下降
	)


