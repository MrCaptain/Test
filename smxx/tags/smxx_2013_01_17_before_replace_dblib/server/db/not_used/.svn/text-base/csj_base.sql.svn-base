  `num` smallint(5) unsigned NOT NULL COMMENT '历史使用数量',
  `fina_time` int(11) unsigned NOT NULL default '0' COMMENT '近最使用时间',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='做玩家的物品使用限制记录,当需要限制玩家在特定时间内使用物品数量时，可使用此表记录';

