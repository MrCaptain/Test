-- ----------------------------
-- Table structure for `base_dungeon`
-- ----------------------------
DROP TABLE IF EXISTS `base_dungeon`;
CREATE TABLE `base_dungeon` (
  `id` int(11) NOT NULL default '0' COMMENT '副本标示ID',
  `mid` int(11) NOT NULL default '0' COMMENT '物群怪ID',
  `sid` int(11) NOT NULL default '0' COMMENT '景场ID',
  `btid` int(11) NOT NULL default '0' COMMENT '斗战场景ID',
  `num` int(11) NOT NULL default '0' COMMENT '数次限制(0表示不限制)',
  `sname` varchar(50) NOT NULL default '' COMMENT '副本点名称',
  `type` int(11) NOT NULL default '0' COMMENT '本副点类型',
  `stype` int(11) NOT NULL default '0' COMMENT '本副子类型(0普通副本，1 boss副本，2精英副本)',
  `pre` int(11) NOT NULL default '0' COMMENT '驱前副本点',
  `task` int(11) NOT NULL default '0' COMMENT '承接指定任务',
  `next` varchar(50) default '' COMMENT '后驱副本点',
  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

