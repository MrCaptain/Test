-- ----------------------------
-- Table structure for `meridian`
-- ----------------------------
DROP TABLE IF EXISTS `meridian`;
CREATE TABLE `meridian` (
  `id` int(11) NOT NULL auto_increment COMMENT '经脉ID',
  `uid` int(11) NOT NULL default '0' COMMENT '玩家ID',
  `mer1` int(11) NOT NULL default '0' COMMENT '百汇（气血）',
  `mer2` int(11) NOT NULL default '0' COMMENT '太渊（普通攻击）',
  `mer3` int(11) NOT NULL default '0' COMMENT '日月（技法估计）',
  `mer4` int(11) NOT NULL default '0' COMMENT '肩井（普通防御）',
  `mer5` int(11) NOT NULL default '0' COMMENT '域中（技法防御）',
  `mer6` int(11) NOT NULL default '0' COMMENT '梁秋（命中）',
  `mer7` int(11) NOT NULL default '0' COMMENT '复溜（格挡）',
  `mer8` int(11) NOT NULL default '0' COMMENT '委中（闪避）',
  `mer9` int(11) NOT NULL default '0' COMMENT '太白（暴击）',
  `clv` int(11) NOT NULL default '0' COMMENT '人物当前修炼值',
  PRIMARY KEY  (`id`),
  KEY `player_id` USING BTREE (`uid`),
  KEY `id` USING BTREE (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 ROW_FORMAT=FIXED;

