DROP TABLE IF EXISTS `base_scene`;
CREATE TABLE `base_scene` (
  `sid` int(11) NOT NULL COMMENT '场景id',
  `type` int(1) NOT NULL default '0' COMMENT '0:城镇, 10:世界Boss副本, 20:联盟领地',
  `name` char(20) NOT NULL default '' COMMENT '场景名称',
  `x` int(11) NOT NULL default '0' COMMENT '默认x坐标',
  `y` int(11) NOT NULL default '0' COMMENT '默认y坐标',
  `npc` text NOT NULL COMMENT '场景NPC,数据结构',
  `id` int(11) NOT NULL default '0' COMMENT '场景唯一ID',
  PRIMARY KEY  (`sid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='场景数据结构';

