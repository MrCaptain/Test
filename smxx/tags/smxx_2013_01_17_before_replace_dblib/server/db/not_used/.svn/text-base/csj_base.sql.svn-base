-- ----------------------------
-- Table structure for `base_mon`
-- ----------------------------
DROP TABLE IF EXISTS `base_mon`;
CREATE TABLE `base_mon` (
  `mtid` int(11) unsigned NOT NULL COMMENT '怪物编号',
  `name` varchar(50) NOT NULL default '' COMMENT '怪物名称',
  `icon` int(11) NOT NULL default '0' COMMENT '标图标示',
  `race` tinyint(1) unsigned NOT NULL default '0' COMMENT '种族(预留)',
  `crr` tinyint(1) unsigned NOT NULL default '0' COMMENT '怪物职业',
  `lv` tinyint(2) unsigned NOT NULL default '0' COMMENT '等级',
  `fexp` double(8,0) unsigned NOT NULL default '0' COMMENT '战斗经验',
  `gexp` double(8,0) unsigned NOT NULL default '0' COMMENT '挂机经验',
  `mxhp` double(8,0) unsigned NOT NULL default '0' COMMENT '最大生命',
  `pwr` int(2) unsigned NOT NULL default '0' COMMENT '内功',
  `tech` int(2) unsigned NOT NULL default '0' COMMENT '技法',
  `mgc` int(2) unsigned NOT NULL default '0' COMMENT '法力',
  `hit` int(2) unsigned NOT NULL default '0' COMMENT '命中',
  `crit` int(2) unsigned NOT NULL default '0' COMMENT '暴击',
  `ddge` int(2) unsigned NOT NULL default '0' COMMENT '闪避',
  `blck` int(2) unsigned NOT NULL default '0' COMMENT '格挡',
  `cter` int(2) unsigned NOT NULL default '0' COMMENT '反击',
  `mana` int(2) unsigned NOT NULL default '50' COMMENT '气势',
  `mnup` int(2) unsigned NOT NULL default '25' COMMENT '气势增加值',
  `mxmn` int(2) NOT NULL default '100' COMMENT '绝技触发气势值',
  `dpwr` int(4) unsigned NOT NULL default '0' COMMENT '内功防御',
  `dtech` int(4) unsigned NOT NULL default '0' COMMENT '技法防御',
  `dmgc` int(4) unsigned NOT NULL default '0' COMMENT '法力防御',
  `apwr` int(4) unsigned NOT NULL default '0' COMMENT '内功攻击',
  `atech` int(4) unsigned NOT NULL default '0' COMMENT '技法攻击',
  `amgc` int(4) unsigned NOT NULL default '0' COMMENT '法力攻击',
  `type` tinyint(1) unsigned NOT NULL default '0' COMMENT '怪物类型，0-普通，1-精英，2-boss',
  `sklid` int(11) unsigned NOT NULL default '0' COMMENT '天赋技能id',
  `dbas` int(4) NOT NULL COMMENT '基础防御',
  `abas` int(4) NOT NULL COMMENT '基础攻击',
  PRIMARY KEY  (`mtid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='宠物属性配置';

