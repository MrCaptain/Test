-- ----------------------------
-- Table structure for `base_target`
-- ----------------------------
DROP TABLE IF EXISTS `base_target`;
CREATE TABLE `base_target` (
  `id` int(11) NOT NULL default '0' COMMENT '目标id',
  `cls` int(11) NOT NULL default '0' COMMENT '阶段',
  `pack` int(3) NOT NULL default '0' COMMENT '主从，0主，1从',
  `type` int(11) NOT NULL default '0' COMMENT '类型',
  `num` int(11) NOT NULL default '0' COMMENT '次数，或者需要达到等级或者数目',
  `coin` int(11) NOT NULL default '0',
  `gold` int(11) NOT NULL default '0',
  `exp` int(11) NOT NULL default '0',
  `stone` varchar(2000) NOT NULL default '[]' COMMENT '晶石',
  `goods` varchar(2000) NOT NULL default '[]' COMMENT '奖励物品',
  `desc` varchar(2000) default NULL COMMENT '描述',
  `name` varchar(500) default NULL COMMENT '名字',
  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`),
  KEY `type` USING BTREE (`type`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

