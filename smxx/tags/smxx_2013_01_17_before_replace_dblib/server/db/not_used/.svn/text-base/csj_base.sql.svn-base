-- Table structure for `sys_acm`
-- ----------------------------
DROP TABLE IF EXISTS `sys_acm`;
CREATE TABLE `sys_acm` (
  `id` int(11) unsigned NOT NULL auto_increment,
  `acm_type` tinyint(4) unsigned NOT NULL default '0' COMMENT '系统公告的类型（0：循环公告和聊天区公告、1：循环公告、2：聊天区公告）',
  `begtime` int(11) unsigned NOT NULL default '0' COMMENT '公告开始时间（unixtime格式、0为立即开始）',
  `endtime` int(11) NOT NULL default '0' COMMENT '系统公告停止时间（unixtime格式、0则永不停止）',
  `acm_ivl` int(11) NOT NULL default '0' COMMENT '公告发送间隔（单位：分、0为不发送）',
  `nexttime` int(11) NOT NULL default '0' COMMENT '下次发送时间（untime格式、0为尚未发送）',
  `content` varchar(200) character set utf8 NOT NULL default '' COMMENT '公告内容',
  `acm_color` char(7) character set utf8 NOT NULL default '#FF0000' COMMENT '公告颜色',
  `acm_link` varchar(200) character set utf8 default '' COMMENT '内置链接',
  `acm_times` int(11) NOT NULL default '0' COMMENT '发送次数（0为不限次数）',
  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=latin1;

