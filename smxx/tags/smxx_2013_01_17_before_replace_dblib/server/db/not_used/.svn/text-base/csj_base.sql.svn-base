DROP TABLE IF EXISTS `base_frmt`;
CREATE TABLE `base_frmt` (
  `bbtid` int(32) NOT NULL default '0' COMMENT '基础阵法ID',
  `nick` varchar(50) character set utf8 default NULL COMMENT '阵法名称',
  `lv` int(8) default NULL COMMENT '阵法等级',
  `bttlt` varchar(300) default NULL COMMENT '阵法技能列表',
  `techv` int(8) default NULL COMMENT '剩余技能点',
  `bdesc` varchar(300) default NULL COMMENT '阵法描述',
  `rbidl` varchar(300) default NULL COMMENT '相克阵型ID列表',
  `posl` varchar(300) default NULL COMMENT '开启位置列表[位格1,位格2,..]',
  `p1` varchar(300) default NULL COMMENT '1号位属性信息',
  `p2` varchar(300) default NULL,
  `p3` varchar(300) default NULL,
  `p4` varchar(300) default NULL,
  `p5` varchar(300) default NULL,
  `p6` varchar(300) default NULL,
  `p7` varchar(300) default NULL,
  `p8` varchar(300) default NULL,
  `p9` varchar(300) default NULL,
  PRIMARY KEY  (`bbtid`)
) ENGINE=MyISAM DEFAULT CHARSET=latin2 COMMENT='基础阵法表';

