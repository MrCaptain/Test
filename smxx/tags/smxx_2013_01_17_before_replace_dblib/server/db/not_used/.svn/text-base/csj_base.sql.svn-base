DROP TABLE IF EXISTS `base_npc`;
CREATE TABLE `base_npc` (
  `nid` int(11) unsigned NOT NULL default '0' COMMENT 'NPC编号',
  `name` varchar(50) NOT NULL default '' COMMENT '字名',
