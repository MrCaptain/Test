DROP TABLE IF EXISTS `base_team_dungeon`;
CREATE TABLE `base_team_dungeon` (
  `dg_id` int(4) NOT NULL default '0' COMMENT '多人副本ID',
  `dg_nm` varchar(50) character set utf8 default '' COMMENT '多人副本名字',
  `max_m` int(4) default '3' COMMENT '最大成员数',
  `times` int(4) default '2' COMMENT '玩家最大参与次数',
  `icon` int(11) default '0' COMMENT '战斗地图ID,不同副本对应不同地图',
  `task` int(11) default '0' COMMENT '对应的主线任务ID',
  `mglst` varchar(255) character set utf8 default '[]' COMMENT '多人副本的怪物群组列表[{mongrp_id,post},....]',
  `award` varchar(255) character set utf8 default '{}' COMMENT '副本奖励{coin,expr,eng,[{goodsid,goodsnumber}]}',
  PRIMARY KEY  (`dg_id`),
  KEY `DG_ICON` USING BTREE (`icon`),
  KEY `DG_TASK` USING BTREE (`task`)
) ENGINE=MyISAM DEFAULT CHARSET=latin1;

