-- ----------------------------
-- Table structure for `base_pet`
-- ----------------------------
DROP TABLE IF EXISTS `base_pet`;
CREATE TABLE `base_pet` (
  `ptid` int(11) unsigned NOT NULL COMMENT '宠物类型id',
  `nick` varchar(50) NOT NULL default '' COMMENT '宠物类型名称',
  `icon` int(11) NOT NULL default '0' COMMENT '标图标示',
  `crr` tinyint(1) unsigned NOT NULL default '0' COMMENT '宠物职业',
  `mxhp` double(8,0) unsigned NOT NULL default '0' COMMENT '最大生命',
  `race` int(5) NOT NULL default '0' COMMENT '种族',
  `qly` smallint(2) NOT NULL default '1' COMMENT '品质，1 白宠， 2蓝宠， 3 橙宠',
  `pwr` int(2) unsigned NOT NULL default '0' COMMENT '内功',
  `tech` int(2) unsigned NOT NULL default '0' COMMENT '技法',
  `mgc` int(2) unsigned NOT NULL default '0' COMMENT '法力',
  `pwup` int(2) unsigned NOT NULL default '0' COMMENT '内功成长值',
  `tcup` int(2) unsigned NOT NULL default '0' COMMENT '技法成长值',
  `mgup` int(2) unsigned NOT NULL default '0' COMMENT '法力增加值',
  `hit` int(2) unsigned NOT NULL default '0' COMMENT '命中',
  `crit` int(2) unsigned NOT NULL default '0' COMMENT '暴击',
  `ddge` int(2) unsigned NOT NULL default '0' COMMENT '闪避',
  `blck` int(2) unsigned NOT NULL default '0' COMMENT '格挡',
  `cter` int(2) unsigned NOT NULL default '0' COMMENT '反击',
  `mana` int(2) unsigned NOT NULL default '0' COMMENT '气势',
  `mnup` int(2) unsigned NOT NULL default '0' COMMENT '气势增加值',
  `dpwr` int(4) unsigned NOT NULL default '0' COMMENT '内功防御',
  `dtech` int(4) unsigned NOT NULL default '0' COMMENT '技法防御',
  `dmgc` int(4) unsigned NOT NULL default '0' COMMENT '法力防御',
  `apwr` int(4) unsigned NOT NULL default '0' COMMENT '内功攻击',
  `atech` int(4) unsigned NOT NULL default '0' COMMENT '技法攻击',
  `amgc` int(4) unsigned NOT NULL default '0' COMMENT '法力攻击',
  `sklid` int(11) unsigned NOT NULL default '0' COMMENT '天赋技能',
  `dcrit` int(2) default '0' COMMENT '防暴击',
  `dblck` int(2) default '0' COMMENT '破格挡',
  `dbas` int(4) default '0' COMMENT '基础防御',
  `abas` int(4) default '0' COMMENT '基础攻击',
  PRIMARY KEY  (`ptid`),
  KEY `index_pet_player_id` USING BTREE (`nick`),
  KEY `id` USING BTREE (`ptid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='宠物基础属性';

