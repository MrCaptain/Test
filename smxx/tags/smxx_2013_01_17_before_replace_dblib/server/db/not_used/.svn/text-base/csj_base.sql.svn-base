DROP TABLE IF EXISTS `log_goods_diff`;
CREATE TABLE `log_goods_diff` (
  `id` int(11) unsigned NOT NULL auto_increment COMMENT '自增ID',
  `time` int(11) unsigned default '0' COMMENT '时间戳',
  `player_id` int(11) unsigned default '0' COMMENT '玩家id',
  `t` tinyint(1) unsigned default '0' COMMENT '检查类型',
  `goods_id` int(11) default '0' COMMENT '品物类型',
  `gid` int(11) unsigned default '0' COMMENT '物品存储id',
  `dnum` int(11) unsigned default '0' COMMENT '数据库数量',
  `mnum` int(11) unsigned default '0' COMMENT '内存数量',
  PRIMARY KEY  (`id`)
