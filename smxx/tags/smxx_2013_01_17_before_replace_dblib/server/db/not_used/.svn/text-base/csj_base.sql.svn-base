DROP TABLE IF EXISTS `battle_cache_data`;
CREATE TABLE `battle_cache_data` (
  `id` bigint(32) NOT NULL default '0' COMMENT '增自ID',
  `actid` bigint(32) default '0' COMMENT '玩家ID',
  `acnm` varchar(20) default NULL COMMENT '击攻方名字',
  `defid` bigint(32) default NULL COMMENT '防守方id，如果为（PVP-防守方玩家ID，PVE-副本ID）',
  `dfnm` varchar(20) default NULL COMMENT '防守方名字',
  `wrmd` int(8) default '0' COMMENT '战斗模式（0-pvp,1-pve)',
  `svty` int(8) default '1' COMMENT '存储类型:(1-限时存储,到期删除,2-永久保存)',
  `stim` bigint(32) default '0' COMMENT '存储时间',
  `lftm` bigint(32) default '0' COMMENT '到期时间:(到期自动删除)',
  `bbin` varbinary(500) default NULL COMMENT '战斗二进制数据',
  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`),
  KEY `stim` USING BTREE (`stim`)
) ENGINE=InnoDB DEFAULT CHARSET=latin2 ROW_FORMAT=COMPACT COMMENT='战斗分享数据';

