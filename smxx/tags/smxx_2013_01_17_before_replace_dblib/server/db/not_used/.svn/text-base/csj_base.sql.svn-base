DROP TABLE IF EXISTS `base_guild_fort`;
CREATE TABLE `base_guild_fort` (
  `gfid` int(11) NOT NULL default '0' COMMENT '联盟战据点标识',
  `gfnm` varchar(20) character set utf8 default '' COMMENT '联盟据点名称',
  `gftp` int(11) default '1' COMMENT '1-小型村庄；2-中型城镇；3-大型城市；4-巨型要塞-----',
  `award` varchar(50) character set utf8 default '[]' COMMENT '据点奖励:[{goodstypeid,number}] ,everything is goods',
  `icon` int(11) default '0' COMMENT '据点图片',
  `map` int(11) default '0' COMMENT '据点战斗地图。',
  `maxg` int(11) default '16' COMMENT '报名参战联盟上限,',
  `glv` int(11) default '1' COMMENT '开放等级。及联盟多少级后才能开放。',
  `numb` int(11) default '1' COMMENT '据点个数：小型村庄4个，中型城镇3个，大型城市2个，巨型要塞1个',
  `appn` int(11) default '0' COMMENT '已经报名联盟数',
  `apply_guilds` varchar(2000) character set utf8 default '[]' COMMENT '已报名联盟组',
  PRIMARY KEY  (`gfid`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1 ROW_FORMAT=COMPACT;

