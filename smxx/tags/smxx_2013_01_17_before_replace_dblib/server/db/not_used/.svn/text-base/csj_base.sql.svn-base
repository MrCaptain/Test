  `base_db_user` varchar(50) NOT NULL default '' COMMENT '基础数据数据库user',
  `base_db_pwd` varchar(50) NOT NULL default '' COMMENT '基础数据数据库password',
  `base_db_db` varchar(50) NOT NULL default '' COMMENT '基础数据数据库db',
  `game_db_type` varchar(50) NOT NULL default 'mysql' COMMENT '主游戏数据库类型(mysql or mongo)',
  `game_db_host` varchar(30) NOT NULL default '' COMMENT '主游戏数据库host',
  `game_db_port` varchar(10) NOT NULL default '' COMMENT '主游戏数据库port',
  `game_db_user` varchar(50) NOT NULL default '' COMMENT '主游戏数据库user',
  `game_db_pwd` varchar(50) NOT NULL default '' COMMENT '主游戏数据库password',
  `game_db_db` varchar(50) NOT NULL default '' COMMENT '主游戏数据库db',
  `game_slave_db_type` varchar(50) NOT NULL default 'mysql' COMMENT '从游戏数据库类型(mysql or mongo)',
  `game_slave_db_host` varchar(30) NOT NULL default '' COMMENT '从游戏数据库host',
  `game_slave_db_port` varchar(10) NOT NULL default '' COMMENT '从游戏数据库port',
  `game_slave_db_user` varchar(50) NOT NULL default '' COMMENT '从游戏数据库user',
  `game_slave_db_pwd` varchar(50) NOT NULL default '' COMMENT '从游戏数据库password',
  `game_slave_db_db` varchar(50) NOT NULL default '' COMMENT '从游戏数据库db',
  `log_db_type` varchar(50) NOT NULL default 'mysql' COMMENT '主日志数据库类型(mysql or mongo)',
  `log_db_host` varchar(30) NOT NULL default '' COMMENT '主日志数据库host',
  `log_db_port` varchar(10) NOT NULL default '' COMMENT '主日志数据库port',
  `log_db_user` varchar(50) NOT NULL default '' COMMENT '主日志数据库user',
  `log_db_pwd` varchar(50) NOT NULL default '' COMMENT '主日志数据库password',
  `log_db_db` varchar(50) NOT NULL default '' COMMENT '主日志数据库db',
  `log_slave_db_type` varchar(50) NOT NULL default 'mysql' COMMENT '从日志数据库类型(mysql or mongo)',
  `log_slave_db_host` varchar(30) NOT NULL default '' COMMENT '从日志数据库host',
  `log_slave_db_port` varchar(10) NOT NULL default '' COMMENT '从日志数据库port',
  `log_slave_db_user` varchar(50) NOT NULL default '' COMMENT '从日志数据库user',
  `log_slave_db_pwd` varchar(50) NOT NULL default '' COMMENT '从日志数据库password',
  `log_slave_db_db` varchar(50) NOT NULL default '' COMMENT '从日志数据库db',
  `logs_url` varchar(300) NOT NULL default '' COMMENT '游戏服务器日志url(可多个，“，”分隔)',
  `enabled` tinyint(1) NOT NULL default '1' COMMENT '是否有效(1--有效；2--无效)',
  `order_num` int(11) NOT NULL default '0' COMMENT '排序数字(越大越靠后)',
  `mark` varchar(200) NOT NULL default '' COMMENT '备注',
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COMMENT='游戏开服信息';

