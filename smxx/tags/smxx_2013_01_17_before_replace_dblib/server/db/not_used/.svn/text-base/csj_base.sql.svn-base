-- ----------------------------
-- Table structure for `base_random_events`
-- ----------------------------
DROP TABLE IF EXISTS `base_random_events`;
CREATE TABLE `base_random_events` (
  `id` int(8) NOT NULL auto_increment COMMENT '自增ID',
  `bbt` int(8) NOT NULL default '0' COMMENT '战斗标识（0-无战斗，1-战斗）',
  `baw` int(8) NOT NULL default '0' COMMENT '奖励标识（0-无奖励，1-有奖励）',
  `bbrc` int(8) NOT NULL default '0' COMMENT '是否广播(0-不广播，1-广播）',
  `dscr` varchar(100) character set utf8 NOT NULL default '' COMMENT '随机事件描述',
  `awlt` varchar(100) NOT NULL default '[]' COMMENT '励奖列表（[{物品ID,物品数量}, ..]),如果发生战斗为战斗胜利奖励',
  `mgid` bigint(32) default '0' COMMENT '物群组怪ID',
  `fawlt` varchar(100) default '[]' COMMENT '战斗失败励奖列表（[{物品ID,物品数量}, ..])',
  `vscr` varchar(100) character set utf8 default '' COMMENT '战斗胜利描述',
  `fscr` varchar(100) character set utf8 default '' COMMENT '战斗失败描述',
  `rate` int(8) NOT NULL default '0' COMMENT '触发概率（数值为整形）',
  PRIMARY KEY  (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=latin2 ROW_FORMAT=COMPACT COMMENT='采集随机事件BASE表';

-- ----------------------------
-- Records of base_random_events
-- ----------------------------
INSERT INTO base_random_events VALUES ('1', '0', '1', '0', '走路踢到石头，捡起来一看，居然是天外陨石，喜滋滋地获得~s', '[{210101, {200, 2000}}]', '0', '[]', '', '', '15');
INSERT INTO base_random_events VALUES ('2', '0', '1', '0', '不小心掉到地洞，痛死了。抬头发现原来掉进了上古遗迹，满载而归，获得~s', '[{210101, {500, 1500}}]', '0', '[]', '', '', '15');
INSERT INTO base_random_events VALUES ('3', '0', '1', '0', '突然刮起一阵风，旁边美女的短裙翩翩起舞，令你浮想联翩，不小心撞上大叔，美女笑得花枝招展，增加~s', '[{210401, 6}]', '0', '[]', '', '', '5');
INSERT INTO base_random_events VALUES ('4', '1', '1', '0', '走路捡到10元宝，但铁刀布长说是他丢的，不管你信不信，反正我不信。进入战斗…', '[{210201, 10}]', '15', '[{210201, 5}]', '你打败了铁刀布长，把他打成了猪头，成功把~s收人囊中', '你被铁刀布长狠狠的教训了，他嚣张的说：”小样的，说我的就是我的，还不信，给你~s吧。“', '5');
INSERT INTO base_random_events VALUES ('5', '0', '1', '0', '天上掉下个林妹妹，你对她如珠如宝。林妹妹临走时送你一神丹，增加~s', '[{210301, {150, 2500}}]', '0', '[]', '', '', '20');
INSERT INTO base_random_events VALUES ('6', '1', '1', '0', '采集过程中，眼前一亮，被卷入黑洞，误入世外桃源，被土著们包围。进入战斗…', '[{210301, {300, 3600}}]', '16', '[{210301, {180, 2000}}]', '把土著们打得落花流水，不幸又卷入黑洞，回到长生战道，发现自己实力大增，增加~s', '突然惊醒，发现自己睡在恐怖图腾旁边，自己胆识大增，增加~s', '15');
INSERT INTO base_random_events VALUES ('7', '0', '1', '0', '带着潇洒哥的忧郁，用最拉风的姿势甩了一下头，被周围群众狂砸各种东西，仔细寻找之后，惊喜获得~s', '[{210101, {400, 1888}}]', '0', '[]', '', '', '15');
INSERT INTO base_random_events VALUES ('8', '0', '1', '0', '发现一龙窝，巨龙刚好不在，你满心欢喜，顺手牵羊，获得1颗暴龙元', '[{132002, 1}]', '0', '[]', '', '', '5');
INSERT INTO base_random_events VALUES ('9', '0', '1', '0', '发现一只受伤的剑龙，你对剑龙悉心照料，临走时，他送你1颗剑龙元', '[{132003, 1}]', '0', '[]', '', '', '5');

-- ----------------------------
-- Table structure for `base_scene`
