INSERT INTO base_target VALUES ('10101', '6', '1', '1', '40', '15000', '0', '0', '[]', '[]', '相识凡几，知心数人。快乐伤心，谁人与共？当我会心一笑，远方的你是否知晓。拥有10个好友。', '添加好友');
INSERT INTO base_target VALUES ('10102', '3', '1', '1', '20', '12000', '0', '0', '[]', '[]', '村镇溪桥，天帝彩霞，殷都晚钟，一路风尘携手走过，总有那么几个身影，萦绕心间。拥有20个好友。', '添加好友');
INSERT INTO base_target VALUES ('10103', '5', '1', '1', '30', '14000', '0', '0', '[]', '[]', '莫愁前路无知己，天下谁人不识君。一个好友，一段故事，一份真情。拥有30个好友。', '添加好友');
INSERT INTO base_target VALUES ('10104', '2', '1', '1', '10', '11000', '0', '0', '[]', '[]', '村镇溪桥，天帝彩霞，殷都晚钟，一路风尘携手走过，总有那么几个身影，萦绕心间。拥有10个好友。', '添加好友');
INSERT INTO base_target VALUES ('10201', '6', '0', '2', '60', '105000', '320', '0', '[]', '[{132003,10}]', '将人物等级升至60级。', '人物达到60级');
INSERT INTO base_target VALUES ('10202', '5', '0', '2', '50', '75000', '260', '0', '[]', '[{132003,5}]', '将人物等级升至50级。', '人物达到50级');
INSERT INTO base_target VALUES ('10203', '4', '0', '2', '40', '50000', '200', '0', '[]', '[{132002,10}]', '将人物等级升至40级。', '人物达到40级');
INSERT INTO base_target VALUES ('10204', '3', '0', '2', '30', '35000', '140', '0', '[]', '[{132002,5}]', '将人物等级升至30级。', '人物达到30级');
INSERT INTO base_target VALUES ('10205', '2', '0', '2', '20', '25000', '80', '0', '[]', '[{132001,10}]', '将人物等级升至20级。', '人物达到20级');
INSERT INTO base_target VALUES ('10206', '1', '0', '2', '10', '20000', '20', '0', '[]', '[{132001,5}]', '将人物等级升至10级。', '人物达到10级');
INSERT INTO base_target VALUES ('10301', '5', '1', '3', '4', '14000', '0', '0', '[]', '[]', '排兵布阵，运筹帷幄，带领宠物军团，冲破一切阻碍，创造更辉煌的未来。（须将阵法升至40级）', '带四只宠物出战');
INSERT INTO base_target VALUES ('10302', '4', '1', '3', '3', '13000', '0', '0', '[]', '[]', '宠物军团扩容咯，更多的宠物，更多的选择，不同站位应对挑战，你HOLD住了吗？（须将阵法升至30级）', '带三只宠物出战');
INSERT INTO base_target VALUES ('10303', '2', '1', '3', '2', '11000', '0', '0', '[]', '[]', '带领你的宠物军团，一起投入激动人心的战斗吧。记住，你不是一个人！（须将阵法升至20级）', '带两只宠物出战');
INSERT INTO base_target VALUES ('10304', '1', '1', '3', '1', '10000', '0', '0', '[]', '[]', '漫漫长生，怎能独自探索，让宠物上阵一起战斗吧！', '和宠物并肩作战');
INSERT INTO base_target VALUES ('10401', '1', '1', '4', '1', '10000', '0', '0', '[]', '[]', '艰苦战斗怎能没有一件像样的装备（将某一装备强化至青铜5级）。', '装备强化');
INSERT INTO base_target VALUES ('10501', '4', '1', '5', '6', '13000', '0', '0', '0', '[]', '行走天下，一身装备给你最贴身的保护。别忘了装备强化，点点更强大！将所有装备强化至黄金5级。', '装备强化');
INSERT INTO base_target VALUES ('10601', '4', '1', '6', '80', '13000', '0', '0', '[]', '[]', '巨兽食量大，喂不饱？求助你的好友吧。投桃报李，别忘了喂好友的巨兽哦。喂食好友巨兽80次', '喂养好友巨兽');
INSERT INTO base_target VALUES ('10602', '1', '1', '41', '1', '10000', '0', '0', '[]', '[]', '让巨兽强大起来，点击巨兽界面的喂食按钮。', '喂养巨兽');
INSERT INTO base_target VALUES ('11001', '5', '1', '10', '1', '14000', '0', '0', '[]', '[]', '宠物是打怪好帮手，PK好搭档，修行好伙伴。拥有一只紫宠作为你的最佳拍档吧。', '拥有一只紫宠');
INSERT INTO base_target VALUES ('11101', '6', '1', '11', '1', '15000', '0', '0', '[]', '[]', '曾经它是高贵的存在，曾经它是幸运儿的专属；如今我终于拥有了它，叫我如何不珍惜。', '拥有一只橙宠');
INSERT INTO base_target VALUES ('11201', '3', '1', '12', '1', '12000', '0', '0', '[]', '[]', '宠物是打怪好帮手，PK好搭档，修行好伙伴。拥有一只蓝宠作为你的最佳拍档吧。', '拥有一只蓝宠');
INSERT INTO base_target VALUES ('11301', '6', '1', '37', '5', '15000', '0', '0', '[]', '[]', '穴道等级越高，提供的实力加成就越多。5个穴位升至5级。', '穴位升级');
INSERT INTO base_target VALUES ('11302', '4', '1', '35', '3', '13000', '0', '0', '[]', '[]', '当穴道拥有足够的神龙元力，就会进步神化，提供种种不可思议的能力。3个穴位升至3级。', '穴位升级');
INSERT INTO base_target VALUES ('11303', '5', '1', '36', '4', '14000', '0', '0', '[]', '[]', '穴道等级越高，提供的实力加成就越多。4个穴位升至4级。', '穴位升级');
INSERT INTO base_target VALUES ('11401', '4', '1', '38', '2', '13000', '0', '0', '0', '[]', '虽然宠物是一堆吃货，但可不是白吃哦，不仅能增加亲密度，更能增强战斗实力。将两只宠物升至4阶。', '四阶宠物');
INSERT INTO base_target VALUES ('11402', '5', '1', '39', '3', '14000', '0', '0', '0', '[]', '虽然宠物是一堆吃货，但可不是白吃哦，不仅能增加亲密度，更能增强战斗实力。将三只宠物升至5阶。', '五阶宠物');
INSERT INTO base_target VALUES ('11403', '6', '1', '40', '4', '15000', '0', '0', '0', '[]', '虽然宠物是一堆吃货，但可不是白吃哦，不仅能增加亲密度，更能增强战斗实力。将四只宠物升至6阶。', '六阶宠物');
INSERT INTO base_target VALUES ('11501', '3', '1', '34', '1', '12000', '0', '0', '[]', '[]', '浮云装跟了你这么久，也该寿终正寝光荣退休了。现在你要用浮云二代——流光套！', '合成流光武器');
INSERT INTO base_target VALUES ('11502', '4', '1', '15', '6', '13000', '0', '0', '[]', '[]', '浮云套彻底成为历史，换上了流光溢彩的新套装，是不是感觉更帅气靓丽了呢？合成六件流光装备。', '六件流光装备');
INSERT INTO base_target VALUES ('11601', '6', '1', '16', '6', '15000', '0', '0', '[]', '[]', '北斗星力灌注装备，可以大幅提升品质和力量。准备好了吗？赶紧合成去吧。合成六件北斗装备。', '六件北斗装备');
INSERT INTO base_target VALUES ('11801', '5', '1', '18', '5', '14000', '0', '0', '[]', '[]', '联盟的强大，需要每一位盟众的建设和贡献。人人参与，联盟的明天才会更辉煌。所在联盟升至5级。', '所在联盟升至5级');
INSERT INTO base_target VALUES ('11802', '6', '1', '18', '8', '15000', '0', '0', '[]', '[]', '失败的联盟各有各的失败，而强大的联盟都是相似的——团结一心，无坚不摧。所在联盟升至8级。', '所在联盟升至8级');
INSERT INTO base_target VALUES ('11901', '6', '1', '19', '1', '15000', '0', '0', '[]', '[]', '竞技场前面的都是高手啊，想打赢不容易啊。嘿嘿，打进前200，我也算高手了吧？竞技场排名前200。', '竞技场排名前200');
INSERT INTO base_target VALUES ('12001', '4', '1', '20', '5', '13000', '0', '0', '[]', '[]', '要想连胜五场，光靠体验卡可不行。需要你整体实力的提升和一点点运气。竞技场挑战连胜5次。', '竞技场连胜5次');
INSERT INTO base_target VALUES ('12201', '2', '1', '22', '1', '11000', '0', '0', '[]', '[]', '点击界面右下角的打坐图标即可切换至打坐状态，轻轻松松经验到手。', '打坐状态切换');
INSERT INTO base_target VALUES ('12401', '3', '1', '24', '20', '12000', '0', '0', '[]', '[]', '修者间的事情，怎么能叫偷呢？你喂我的兽，我偷你的蛋，礼尚往来，哟呵呵~~砸取好友圣蛋20个。', '砸好友圣蛋');
INSERT INTO base_target VALUES ('12601', '2', '1', '18', '1', '11000', '0', '0', '[]', '[]', '团结才是力量之源，加入联盟，体验互助的情谊，感受团战的激情。', '加入或创建联盟');
INSERT INTO base_target VALUES ('12701', '2', '1', '27', '2', '11000', '0', '0', '[]', '[]', '砸蛋可以获得龙元、铜钱等。一天一窝记得收蛋，不然就被好友砸光了。', '收获圣蛋2窝');
INSERT INTO base_target VALUES ('12801', '3', '1', '28', '1', '12000', '0', '0', '[]', '[]', '旅途尚未终结，英雄等着归来，奈何体力不支？购买体力吧，给你继续挑战的动力！', '购买体力一次');
INSERT INTO base_target VALUES ('13001', '3', '1', '30', '1', '12000', '0', '0', '[]', '[]', '芭比巨龙血洗人间？身为修士，我必须去阻止它！若能击杀BOSS，我就是大英雄啦。', '参加世界boss');
