  `old` int(11) unsigned NOT NULL default '0' COMMENT '老玩家（连续2天登录）',
  `reg` int(11) unsigned NOT NULL default '0' COMMENT '注册人数',
  `ltime` int(11) unsigned NOT NULL default '0' COMMENT '统计时间',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='服务器登录统计';

