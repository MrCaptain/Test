-- ----------------------------
-- Table structure for `task_daily`
-- ----------------------------
DROP TABLE IF EXISTS `task_daily`;
CREATE TABLE `task_daily` (
  `id` int(11) NOT NULL default '0' COMMENT 'id',
  `uid` int(11) NOT NULL default '0' COMMENT '家玩ID',
  `num` int(11) NOT NULL default '0' COMMENT '完成可次数',
  `use` int(11) NOT NULL default '0' COMMENT '完成已次数',
  `frtm` int(11) NOT NULL default '0' COMMENT '免费可次数',
  `frus` int(11) NOT NULL default '0' COMMENT '已用免费次数',
  `rstm` int(11) NOT NULL default '0' COMMENT '重置时间',
  `lstm` int(11) NOT NULL default '0' COMMENT '每天更新时间',
  `rftm` int(11) NOT NULL default '0' COMMENT '刷新时间',
  `cont` varchar(250) default '[]' COMMENT '日常任务内容',
  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`),
  KEY `player_id` USING BTREE (`uid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

