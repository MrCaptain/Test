  `ddge` smallint(5) unsigned NOT NULL default '0' COMMENT '躲避',
  `crit` smallint(5) unsigned NOT NULL default '0' COMMENT '暴击',
  `blck` smallint(5) unsigned NOT NULL default '0' COMMENT '格挡',
  `cter` smallint(5) unsigned NOT NULL default '0' COMMENT '反击',
  `mana` smallint(5) unsigned NOT NULL default '50' COMMENT '气势',
  `mnup` smallint(5) NOT NULL default '25' COMMENT '气势增加值',
  `ttl` varchar(50) NOT NULL default '' COMMENT '称号',
