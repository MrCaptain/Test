-- Table structure for `base_daily_task`
-- ----------------------------
DROP TABLE IF EXISTS `base_daily_task`;
CREATE TABLE `base_daily_task` (
  `id` int(11) NOT NULL auto_increment COMMENT 'id',
  `name` varchar(30) NOT NULL default '' COMMENT '称名',
  `desc` text NOT NULL COMMENT '描述',
  `qly` int(11) NOT NULL default '0' COMMENT '品质',
  `type` int(3) NOT NULL default '0' COMMENT '种类',
  `lv` int(11) NOT NULL default '0' COMMENT '最低等级限制',
  `lvlt` int(11) NOT NULL default '0' COMMENT '最高等级限制',
  `exp` int(11) NOT NULL default '0' COMMENT '经验 (任务完成的奖励)',
  `coin` int(11) NOT NULL default '0' COMMENT '铜币',
  `gold` int(11) NOT NULL default '0' COMMENT '元宝',
  `cont` int(11) NOT NULL default '0' COMMENT '内容',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=503056369 DEFAULT CHARSET=utf8 ROW_FORMAT=DYNAMIC;

