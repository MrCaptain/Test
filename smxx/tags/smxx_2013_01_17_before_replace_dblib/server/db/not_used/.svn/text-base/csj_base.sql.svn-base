-- ----------------------------
-- Table structure for `log_notice`
-- ----------------------------
DROP TABLE IF EXISTS `log_notice`;
CREATE TABLE `log_notice` (
  `id` int(11) NOT NULL,
  `uid` int(11) default NULL,
  `claz` tinyint(2) default '0' COMMENT '通知类型：1-系统补偿；2-登陆奖励，3-世界BOSS，4-新手奖励，5-竞技场，6-天宫探宝，7-联盟站,8-首冲礼包',
  `type` tinyint(2) default '0' COMMENT '消息类型(0-系统通知，1-竞技场，2-联盟，3-活动，4-好友, 5-世界BOSS,6-祝福,99 -系统发给个人的邮件',
  `cntt` varchar(4000) character set utf8 default NULL COMMENT '消息内容',
  `tmsp` int(11) default NULL COMMENT '消息生成时间戳',
  `exp` int(11) default '0' COMMENT '附件：体力',
  `eng` int(11) default '0' COMMENT '附件：经验',
  `coin` int(11) default '0' COMMENT '附件：铜钱',
  `prstg` int(11) default '0' COMMENT '战勋',
  `gold` int(11) default '0' COMMENT '附件：元宝',
  `goods` varchar(255) character set utf8 default '[]' COMMENT '附件：物品[{goodtypeid,number},...]',
  `sts` tinyint(2) default '1' COMMENT '状态：-1-审核不通过，0-未审核，1-未领取，2-已领取',
  `rtmsp` int(11) default '0' COMMENT '消息读取时间戳',
  `otid` int(11) default '0' COMMENT '其它ID，例如好友ID',
  PRIMARY KEY  (`id`),
  KEY `uid` USING BTREE (`uid`)
) ENGINE=InnoDB DEFAULT CHARSET=cp850;

-- ----------------------------
-- Records of log_notice
-- ----------------------------

-- ----------------------------
-- Table structure for `log_offline_award`
-- ----------------------------
DROP TABLE IF EXISTS `log_offline_award`;
CREATE TABLE `log_offline_award` (
  `id` int(11) NOT NULL auto_increment,
  `pid` int(11) NOT NULL default '0' COMMENT '玩家id',
  `hour` tinyint(8) NOT NULL default '0' COMMENT '兑换小时',
  `mult` tinyint(8) NOT NULL default '0' COMMENT '兑换倍数',
  `exp` int(11) NOT NULL default '0' COMMENT '兑换经验',
  `timestamp` bigint(20) NOT NULL default '0' COMMENT '兑换时间',
  PRIMARY KEY  (`id`),
  KEY `pid` USING BTREE (`pid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='玩家兑换离线凝神日志';

-- ----------------------------
-- Records of log_offline_award
-- ----------------------------

-- ----------------------------
-- Table structure for `log_online_gift`
-- ----------------------------
DROP TABLE IF EXISTS `log_online_gift`;
CREATE TABLE `log_online_gift` (
  `id` int(11) NOT NULL auto_increment,
  `uid` int(11) NOT NULL default '0' COMMENT '玩家ID',
  `num` int(11) NOT NULL default '0',
  `gold` int(11) NOT NULL default '0',
  `times` int(11) NOT NULL default '0',
  `stamp` bigint(20) NOT NULL default '0',
