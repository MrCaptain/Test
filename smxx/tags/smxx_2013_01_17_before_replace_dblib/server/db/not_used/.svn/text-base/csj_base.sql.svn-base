-- ----------------------------
-- Table structure for `mon_drop_analytics`
-- ----------------------------
DROP TABLE IF EXISTS `mon_drop_analytics`;
CREATE TABLE `mon_drop_analytics` (
  `id` int(11) NOT NULL auto_increment,
  `mon_id` int(11) unsigned NOT NULL COMMENT '怪物ID ',
  `mon_name` varchar(50) NOT NULL COMMENT '怪物名',
  `player_id` int(11) unsigned NOT NULL COMMENT '玩家ID',
  `player_name` varchar(50) NOT NULL COMMENT '玩家名',
  `goods_id` int(11) unsigned NOT NULL COMMENT '物品ID',
  `goods_name` varchar(50) NOT NULL COMMENT '物品名',
  `goods_color` tinyint(1) unsigned NOT NULL COMMENT '物品品质',
  `drop_time` int(11) unsigned NOT NULL COMMENT '掉落时间',
  PRIMARY KEY  (`id`),
  KEY `mon_id` USING BTREE (`mon_id`),
  KEY `player_id` USING BTREE (`player_id`),
  KEY `player_name` USING BTREE (`player_name`),
  KEY `goods_id` USING BTREE (`goods_id`),
  KEY `goods_name` USING BTREE (`goods_name`),
  KEY `goods_color` USING BTREE (`goods_color`),
  KEY `id` USING BTREE (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='怪物掉落统计表';

-- ----------------------------
-- Records of mon_drop_analytics
