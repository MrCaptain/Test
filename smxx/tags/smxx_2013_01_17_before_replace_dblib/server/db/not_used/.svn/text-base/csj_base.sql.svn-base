DROP TABLE IF EXISTS `base_online_gift`;
CREATE TABLE `base_online_gift` (
  `id` int(20) NOT NULL auto_increment,
  `amt` int(8) NOT NULL default '0' COMMENT ' 数量',
  `gold` int(11) NOT NULL default '0' COMMENT '励奖元宝',
  `lv` int(20) NOT NULL default '0' COMMENT '等级',
  `times` tinyint(8) NOT NULL default '0' COMMENT '第几份',
  `stamp` bigint(20) NOT NULL default '0' COMMENT ' 时间间隔',
  PRIMARY KEY  (`id`),
  KEY `level` USING BTREE (`lv`,`times`)
) ENGINE=MyISAM AUTO_INCREMENT=8 DEFAULT CHARSET=latin1 ROW_FORMAT=FIXED COMMENT='在线奖励物品表';

