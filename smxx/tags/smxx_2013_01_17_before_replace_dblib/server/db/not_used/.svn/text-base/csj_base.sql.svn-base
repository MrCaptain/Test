-- ----------------------------
-- Table structure for `base_frmttech`
-- ----------------------------
DROP TABLE IF EXISTS `base_frmttech`;
CREATE TABLE `base_frmttech` (
  `bttid` int(32) NOT NULL default '0' COMMENT '阵法技能ID',
  `bbtid` int(32) default NULL COMMENT '基础阵法ID',
  `tname` varchar(50) character set utf8 default NULL COMMENT '法阵技能名称',
  `btlv` int(8) default NULL COMMENT '技能等级',
  `bdesc` varchar(300) default NULL COMMENT '技能描述',
  `limlv` int(8) default NULL COMMENT '等级限制(阵法等级/人物等级)',
  `techp` int(8) default NULL COMMENT '消耗技能点数',
  `posl` varchar(16) default NULL COMMENT '受影响位格列表[位格1,位格2,..]',
  `possw` int(8) default NULL COMMENT '位格开关',
  `hit` int(8) default NULL COMMENT '命中',
  `crit` int(8) default NULL COMMENT '暴击',
  `ddge` int(8) default NULL COMMENT '闪避',
  `blck` int(8) default NULL COMMENT '格挡',
  `cter` int(8) default NULL COMMENT '反击',
  `mana` int(8) default NULL COMMENT '气势',
  `roll` int(8) default NULL COMMENT '碾压',
  `relar` int(8) default NULL COMMENT '连携(亲密度)',
  `dbas` int(32) default NULL COMMENT '基础防御',
  `dpwr` int(32) default NULL COMMENT '内功防御',
  `dtech` int(32) default NULL COMMENT '技法防御',
  `dmgc` int(32) default NULL COMMENT '法力防御',
  `abas` int(32) default NULL COMMENT '基础攻击',
  `apwr` int(32) default NULL COMMENT '内功攻击',
  `atech` int(32) default NULL COMMENT '技法攻击',
  `amgc` int(32) default NULL COMMENT '法力攻击',
  `dcrit` int(32) default '0' COMMENT '防暴击',
  `dblck` int(32) default '0' COMMENT '破格挡',
  PRIMARY KEY  (`bttid`)
) ENGINE=MyISAM DEFAULT CHARSET=latin2 COMMENT='阵法技能基础表';

