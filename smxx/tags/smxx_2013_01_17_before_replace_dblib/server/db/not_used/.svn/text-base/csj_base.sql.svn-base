-- ----------------------------
-- Table structure for `base_task`
-- ----------------------------
DROP TABLE IF EXISTS `base_task`;
CREATE TABLE `base_task` (
  `id` int(11) NOT NULL COMMENT '任务id',
  `name` varchar(30) NOT NULL default '' COMMENT '任务名',
  `desc` text NOT NULL COMMENT '描述',
  `class` smallint(5) NOT NULL default '0' COMMENT '任务分类，0普通任务，1运镖任务，2帮会任务',
  `type` int(2) unsigned NOT NULL default '0' COMMENT '任务类型(主线，支线，日常)',
  `child` int(2) unsigned NOT NULL default '0' COMMENT '子类',
  `kind` int(2) unsigned NOT NULL default '0' COMMENT '任务种类',
  `level` int(10) unsigned NOT NULL default '1' COMMENT '最低等级限制',
  `level_limit` int(10) unsigned NOT NULL default '0' COMMENT '最高等级限制',
  `repeat` tinyint(1) NOT NULL default '0' COMMENT '是否允许重复做',
  `camp` int(1) NOT NULL default '0' COMMENT '阵营',
  `career` tinyint(1) NOT NULL default '0' COMMENT '职业限制',
  `accept_time` text NOT NULL COMMENT '可接时间[Type,"",StartTime,EndTime]',
  `prev` int(11) NOT NULL default '0' COMMENT '上一个必须完成的任务id',
  `next` int(11) NOT NULL default '0' COMMENT '下个关联任务id，即这个任务完成后触发任务',
  `start_item` text NOT NULL COMMENT '开始获得物品{ItemId, Number}',
  `end_item` text NOT NULL COMMENT '完成回收物品',
  `start_npc` text NOT NULL COMMENT '开始npcid',
  `end_npc` int(11) NOT NULL default '0' COMMENT '结束npcid',
  `start_talk` int(11) NOT NULL default '0' COMMENT '开始npc对话',
  `end_talk` int(11) NOT NULL default '0' COMMENT '结束npc对话',
  `unfinished_talk` int(11) NOT NULL default '0' COMMENT '未完成对话',
  `condition` text NOT NULL COMMENT '任务触发条件',
  `content` text NOT NULL COMMENT '任务内容',
  `talk_item` text NOT NULL COMMENT '对话获得物品',
  `state` int(1) unsigned NOT NULL default '0' COMMENT '完成任务需要的状态值 state = length(content)',
  `exp` int(1) unsigned NOT NULL default '0' COMMENT '经验 (任务完成的奖励)',
  `coin` int(1) unsigned NOT NULL default '0' COMMENT '铜币',
  `gold` int(11) NOT NULL COMMENT '元宝',
  `award_item` text NOT NULL COMMENT '奖励物品',
  `award_select_item` text NOT NULL COMMENT '奖励可选物品',
  `award_select_item_num` int(11) NOT NULL default '0' COMMENT '可选奖励物品数量',
  `award_gift` text NOT NULL COMMENT '礼包奖励',
  `start_cost` int(11) unsigned NOT NULL default '0' COMMENT '触发扣除铜币',
  `end_cost` int(11) unsigned NOT NULL default '0' COMMENT '完成扣除铜币',
  `next_cue` int(11) NOT NULL default '0' COMMENT '是否提示下一任务',
  `realm_honor` int(11) NOT NULL default '0' COMMENT '部落荣誉',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='任务数据';

