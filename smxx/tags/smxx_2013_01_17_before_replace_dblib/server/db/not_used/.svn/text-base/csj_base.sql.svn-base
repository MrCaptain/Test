  `upet` int(3) NOT NULL default '1' COMMENT '优宠使用剩余次数',
  `cd` int(11) NOT NULL default '0' COMMENT '冷却时间',
  `rw` int(11) NOT NULL default '0' COMMENT '是否领取过奖励 未领取记录领取的名次 0领取了',
