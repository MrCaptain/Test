-- Procedure structure for `proc_delete_player`
-- ----------------------------
DROP PROCEDURE IF EXISTS `proc_delete_player`;
DELIMITER ;;
CREATE DEFINER=`root`@`127.0.0.1` PROCEDURE `proc_delete_player`(IN `playerid` INT)
begin



		update guild set deputy_chief1_id = 0 where deputy_chief1_id = playerid;



		update guild set deputy_chief2_id = 0 where deputy_chief2_id = playerid;



		delete from guild_apply where guild_id in (select id from guild where chief_id = playerid);



		delete from guild_invite where guild_id in (select id from guild where chief_id = playerid);



		delete from guild_skills_attribute where guild_id in (select id from guild where chief_id = playerid);



		delete from log_guild where guild_id in (select id from guild where chief_id = playerid);



		delete from guild where chief_id = playerid;



		delete from guild_apply where player_id = playerid;



		delete from guild_invite where player_id = playerid;



		delete from relationship  where idA= playerid or idB= playerid;



		delete from master_apprentice  where apprentenice_id= playerid or master_id= playerid;



		delete from master_charts  where master_id = playerid;



		delete from meridian where player_id = playerid;



		delete from sale_goods where player_id = playerid;



		delete from online_gift where player_id = playerid;



		delete from task_bag  where player_id = playerid;



		delete from task_log where player_id = playerid;

		delete from log_use where player_id = playerid;

		delete from log_practice where player_id = playerid;

		delete from log_shop where player_id = playerid;

		delete from goods_attribute where player_id = playerid;



		delete from goods where player_id = playerid;



		delete from goods_cd where player_id = playerid;



		delete from log_backout where player_id = playerid;



		delete from log_compose where player_id = playerid;



		delete from log_hole where player_id = playerid;



		delete from log_inlay where player_id = playerid;



		delete from log_quality_out where player_id = playerid;



		delete from log_quality_up where player_id = playerid;



		delete from log_stren where player_id = playerid;

		delete from log_box_open where player_id = playerid;

		delete from log_wash where player_id = playerid;

		delete from log_dungeon where player_id = playerid;

		delete from player_sys_setting where player_id = playerid;

		delete from player_buff where player_id = playerid;


		delete from player_donttalk where player_id = playerid;



		delete from mail where uid = playerid or sname in (select nickname from player where id = playerid);



		delete from pet where player_id = playerid;



		delete from skill where player_id = playerid;

		delete from target_gift where player_id = playerid;

                delete from goods_buff where player_id = playerid;



		delete from player where id = playerid;



		commit;



end
;;
DELIMITER ;

-- ----------------------------
-- Procedure structure for `proc_delete_player_all`
-- ----------------------------
DROP PROCEDURE IF EXISTS `proc_delete_player_all`;
DELIMITER ;;
CREATE DEFINER=`root`@`127.0.0.1` PROCEDURE `proc_delete_player_all`()
begin



		DECLARE _uid int;



		DECLARE i_error int default 0;



		DECLARE rs_cursor CURSOR FOR select id from `player`;



		DECLARE CONTINUE HANDLER FOR NOT FOUND SET i_error=1;



		OPEN rs_cursor;







		FETCH rs_cursor into _uid;



		REPEAT



			if _uid is not null then



				call proc_delete_player(_uid);



			end if;



			FETCH rs_cursor into _uid;



		UNTIL  i_error=1



		END REPEAT;



		CLOSE rs_cursor;



end
;;
DELIMITER ;
