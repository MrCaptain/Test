CREATE TABLE `log_hunt_pet` (
  `id` int(11) NOT NULL default '0' COMMENT '自增ID',
  `uid` int(11) NOT NULL default '0' COMMENT '家玩ID',
  `ptid` int(11) NOT NULL default '0' COMMENT '宠物类型ID',
  `tlid` smallint(5) NOT NULL default '0' COMMENT '赋天ID',
