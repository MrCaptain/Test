  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`),
  KEY `mon_id` USING BTREE (`mon_id`),
  KEY `goods_id` USING BTREE (`goods_id`)
) ENGINE=MyISAM AUTO_INCREMENT=3112 DEFAULT CHARSET=utf8 COMMENT='物品掉落规则表';

