-- ----------------------------
-- Table structure for `base_goods`
-- ----------------------------
DROP TABLE IF EXISTS `base_goods`;
CREATE TABLE `base_goods` (
  `gtid` int(11) unsigned NOT NULL default '0' COMMENT '物品类型编号',
  `name` varchar(50) NOT NULL default '' COMMENT '物品名称',
  `type` mediumint(8) unsigned NOT NULL default '0' COMMENT '物品类型：10装备类，15宝石类，20护符类，25丹药类，30增益类，35灵兽类，40帮派类，45任务类，50其他类；参照base_goods_type表',
  `stype` mediumint(8) unsigned NOT NULL default '0' COMMENT '物品子类型。装备子类型：1 武器，2 衣服，3 头盗，4 手套，5 鞋子，6 项链，7 戒指。增益子类型：1 药品，2 经验。 坐骑子类型：1 一人坐骑 2 二人坐骑 3 三人坐骑；参照base_goods_subtype表',
  `qly` mediumint(8) NOT NULL COMMENT '品质，决定了物品名称颜色',
  `intro` text NOT NULL COMMENT '物品描述信息',
  `mxnum` mediumint(8) unsigned NOT NULL default '0' COMMENT '可叠加数，0为不可叠加',
  `drp` tinyint(1) unsigned NOT NULL default '0' COMMENT '是否丢弃，0可丢弃，1不可丢弃',
  `ptp` tinyint(1) unsigned NOT NULL default '0' COMMENT '价格类型：1 铜钱, 2 银两，3 金币，4 绑定的铜钱',
  `gpri` int(11) unsigned NOT NULL default '0' COMMENT '物品购买价格',
  `spri` int(11) unsigned NOT NULL default '0' COMMENT '物品出售价格',
  `lv` mediumint(8) unsigned NOT NULL default '0' COMMENT '等级限制，0为不限',
  `crr` smallint(5) unsigned NOT NULL default '0' COMMENT '职业限制，0为不限',
  `stid` mediumint(8) unsigned NOT NULL default '0' COMMENT '套装ID，0为不是套装',
  `mxstr` smallint(5) NOT NULL default '0' COMMENT '最大强化等级，0为不可强化',
  `other_data` text COMMENT '用于保存额外的数据，比如灵兽粮食的快乐值，礼包中的一些赠送物品ID，金币数据\r\n',
  `mxtm` smallint(5) NOT NULL COMMENT '最大使用次数',
  `eprt` bigint(20) unsigned NOT NULL default '0' COMMENT '有效期，0为不限，单位为秒',
  `spec` text COMMENT '特殊字段',
  `atbt` varchar(50) default '[]' COMMENT '装备属性',
  `icon` int(11) NOT NULL default '0' COMMENT '图标id',
  PRIMARY KEY  (`gtid`),
  UNIQUE KEY `gid` (`gtid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='物品基础表';

