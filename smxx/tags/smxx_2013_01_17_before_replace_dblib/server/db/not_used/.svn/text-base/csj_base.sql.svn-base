-- ----------------------------
-- Table structure for `task_bag`
-- ----------------------------
DROP TABLE IF EXISTS `task_bag`;
CREATE TABLE `task_bag` (
  `id` int(11) NOT NULL auto_increment COMMENT '唯一标识id(运行时生成)',
  `uid` int(11) NOT NULL COMMENT '角色id',
  `tid` int(11) NOT NULL default '0' COMMENT '任务id',
  `ttm` int(11) NOT NULL default '0' COMMENT '触发时间',
  `state` int(11) NOT NULL default '0' COMMENT '任务状态',
  `endst` int(11) NOT NULL default '0' COMMENT '结束状态',
  `mark` text NOT NULL COMMENT '任务记录器格式[State=int(),Finish=bool(), Type=atom((), ...]',
  `type` tinyint(4) NOT NULL default '0' COMMENT '任务类型',
  PRIMARY KEY  (`id`),
  KEY `role_id_task_id` USING BTREE (`uid`,`tid`),
  KEY `role_id` USING BTREE (`uid`),
  KEY `task_id` USING BTREE (`tid`),
  KEY `id` USING BTREE (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='角色任务记录';

