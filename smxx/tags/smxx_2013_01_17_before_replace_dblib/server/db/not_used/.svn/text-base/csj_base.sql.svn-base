  `unique_key` smallint(5) NOT NULL default '0' COMMENT 'unique_key用于ets的记录的唯一标记',
  PRIMARY KEY  (`nid`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='NPC表 ';

