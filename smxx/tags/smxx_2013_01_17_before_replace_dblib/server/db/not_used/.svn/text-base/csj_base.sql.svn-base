DROP TABLE IF EXISTS `base_giant`;
CREATE TABLE `base_giant` (
  `id` int(11) unsigned NOT NULL auto_increment COMMENT '巨兽类型编号',
  `nick` varchar(50) NOT NULL default '' COMMENT '巨兽名称',
  `type` mediumint(8) unsigned NOT NULL default '1' COMMENT '种类. 种类1对应的名称"雷兽"对应的技能"雷鸣电闪";种类2:不死之王:屠尽苍生;种类3:八臂恶龙:亢龙有悔;种类4:雪玉龙:飞龙在天;种类5:青色暴龙:神龙摆尾;种类6:狮王龙:龙战于野',
  `technm` varchar(50) NOT NULL default '' COMMENT '技能名称',
  `crr` smallint(5) unsigned NOT NULL default '1' COMMENT '职业',
  `curv` int(11) NOT NULL default '0' COMMENT '当前时间成长值',
  `lvv` int(11) NOT NULL default '0' COMMENT '等级成长值上限',
  `techv` int(11) NOT NULL default '0' COMMENT '技法攻击值',
  `lv` mediumint(8) unsigned NOT NULL default '0' COMMENT '等级',
  `full` int(11) NOT NULL default '0' COMMENT '食度饱',
  `happy` int(11) NOT NULL default '0' COMMENT '开心度',
  `clean` int(11) NOT NULL default '0' COMMENT '清洁度',
  `active` int(11) NOT NULL default '0' COMMENT '活跃度',
  `mood` int(11) NOT NULL default '0' COMMENT '情心值',
  `msid` int(11) default '0' COMMENT '龙鞍ID',
  `msiid` int(11) default '0' COMMENT '龙鞍形象ID',
  `giid` int(11) default '0' COMMENT '巨兽形象ID',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=10 DEFAULT CHARSET=utf8 COMMENT='巨兽类型表';

-- ----------------------------
-- Records of base_giant
