-- ----------------------------
-- Table structure for `base_cooldown`
-- ----------------------------
DROP TABLE IF EXISTS `base_cooldown`;
CREATE TABLE `base_cooldown` (
  `tid` int(11) unsigned NOT NULL COMMENT '类型id',
  `con` int(3) NOT NULL default '0' COMMENT '是否固定CD，0固定，1不固定',
  `type` int(3) NOT NULL default '0' COMMENT 'cd类型，是否爆cd，0不是，1是',
  `low` int(11) NOT NULL default '0' COMMENT '爆cd的最低冷却时间线，单位分钟',
  `clear` int(3) NOT NULL default '0' COMMENT '是否可以清楚cd，0可以，1不可以',
  `cdo` int(11) NOT NULL default '0' COMMENT '固定冷却时间的单次冷却时间',
  `gt` int(11) NOT NULL default '0' COMMENT '清楚cd时最低计算时间，分钟',
  `gold` int(11) NOT NULL default '0' COMMENT '最低计算单位元宝数',
  `desc` varchar(60) default '注释' COMMENT '类型注释',
  PRIMARY KEY  (`tid`),
  KEY `tid` USING BTREE (`tid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

