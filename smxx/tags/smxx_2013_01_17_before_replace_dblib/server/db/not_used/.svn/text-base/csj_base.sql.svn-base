  `eng` int(11) unsigned NOT NULL default '40' COMMENT '体力',
  `engtm` bigint(32) NOT NULL default '0' COMMENT '力体增加时间点',
  `pwr` int(11) unsigned NOT NULL default '0' COMMENT '内功',
  `tech` int(11) unsigned NOT NULL default '0' COMMENT '技法',
  `mgc` int(11) unsigned NOT NULL default '0' COMMENT '法力',
  `apwr` int(11) unsigned NOT NULL default '0' COMMENT '内功攻击',
  `atech` int(11) unsigned NOT NULL default '0' COMMENT '技法攻击',
  `amgc` int(11) unsigned NOT NULL default '0' COMMENT '法力攻击',
  `dpwr` int(11) NOT NULL default '0' COMMENT '内功防御',
  `dtech` int(11) unsigned NOT NULL default '0' COMMENT '技法防御',
  `dmgc` int(11) unsigned NOT NULL default '0' COMMENT '法力防御',
