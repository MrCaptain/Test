  `fg` int(3) NOT NULL default '0' COMMENT '是否提示标志位',
  `bl` varchar(10000) NOT NULL default '[]' COMMENT '战斗记录[{对方id,nick,变化后的排名,是否由自己发起挑战0自己发起，胜负标志0胜利，时间，战斗记录,第x名之争},...]',
  PRIMARY KEY  (`id`,`uid`),
  KEY `id` USING BTREE (`id`),
  KEY `uid` USING BTREE (`uid`)
