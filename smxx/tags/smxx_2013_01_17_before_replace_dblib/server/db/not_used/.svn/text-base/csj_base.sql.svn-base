-- ----------------------------
-- Table structure for `base_mongroup`
-- ----------------------------
DROP TABLE IF EXISTS `base_mongroup`;
CREATE TABLE `base_mongroup` (
  `id` int(32) unsigned NOT NULL COMMENT '物怪群组ID',
  `posl` varchar(300) NOT NULL default '[]' COMMENT '怪物列表[{怪物ID,位格},..]',
  `icon` int(11) NOT NULL default '0' COMMENT '标标示图',
  `coin` int(32) unsigned NOT NULL default '0' COMMENT '铜币奖励',
  `exp` int(32) unsigned NOT NULL default '0' COMMENT '经验',
  `frmid` int(32) unsigned NOT NULL COMMENT '基础阵法类型ID',
  `goods` varchar(300) default NULL COMMENT '物品奖励:[{物品类型ID,概率},..]',
  `desc` text character set utf8 NOT NULL COMMENT '怪物群描述',
  `goth` int(32) NOT NULL default '0' COMMENT '灵力',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=latin2;

