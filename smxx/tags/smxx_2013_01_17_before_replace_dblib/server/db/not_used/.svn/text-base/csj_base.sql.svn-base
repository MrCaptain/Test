-- ----------------------------
-- Table structure for `base_skill`
-- ----------------------------
DROP TABLE IF EXISTS `base_skill`;
CREATE TABLE `base_skill` (
  `id` int(10) unsigned NOT NULL COMMENT '编号',
  `name` varchar(50) NOT NULL default '' COMMENT '技能名称',
  `desc` varchar(200) NOT NULL default '' COMMENT '技能描述',
  `crr` int(11) unsigned NOT NULL default '0' COMMENT '1~3玩家职业，10宠物，11巨兽',
  `imef` int(11) unsigned NOT NULL default '0' COMMENT '技能即时效果类型(每10个id表示一类效果，如1～9表示增加攻击，10～19表示增加闪避 ……）',
  `imvl` int(11) NOT NULL default '0' COMMENT '即时效果数值',
  `immod` int(11) unsigned NOT NULL default '1' COMMENT '即时模式: 1敌方单体/2敌方单排/3敌方单列/4敌方全体/5影杀/6己方单体/7己方全体',
  `bftp` int(11) unsigned NOT NULL default '0' COMMENT 'buff类型，0为无buff',
  `bfval` int(11) NOT NULL COMMENT 'buff数值',
  `bfrg` int(11) unsigned NOT NULL default '0' COMMENT 'buff的概率，0～100',
  `bftm` int(11) unsigned NOT NULL default '0' COMMENT 'buff 持续回合数',
  `bfmod` int(11) unsigned NOT NULL default '1' COMMENT 'buf模式: 1己方单体/2己方全体/3敌方单体/敌方全体',
  `prstg` int(11) NOT NULL default '0' COMMENT '需要的声望',
  `coin` int(11) NOT NULL default '0' COMMENT '铜钱',
  `lv` int(11) NOT NULL default '0' COMMENT '开放等级',
  PRIMARY KEY  (`id`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='技能数据';

