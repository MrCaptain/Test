  `lv` int(11) unsigned default '0' COMMENT '联盟等级(1-20共20个级)，初始为最大42人，每增加以及，人数增加2人，最多为80人。',
  `curn` int(11) unsigned default '0' COMMENT '联盟当前人数,当有成员加入时，更新该字段 current number',
  `maxn` int(11) unsigned default '42' COMMENT '联盟最大人数，等级没升1级，该字段值加2 max member number',
  `dptn` int(11) NOT NULL default '0' COMMENT '前当长老数',
  `devo` int(11) unsigned default '0' COMMENT '联盟贡献度(成员贡献度的总和) devotion',
  `fund` int(11) unsigned default '0' COMMENT '联盟资金(联盟资金可通过成员捐献铜币，完成联盟任务增加。)',
  `flag` int(11) NOT NULL COMMENT '联盟旗帜',
  `updt` int(11) unsigned NOT NULL default '0' COMMENT '最近升级时间 update time',
  `crtt` int(11) unsigned default '0' COMMENT '创建时间 create time',
  `sts` int(1) unsigned default '0' COMMENT '----1 弹劾盟主中，0----正常  在本次弹劾结束时清零',
  `acuid` int(11) unsigned default NULL COMMENT '发起弹劾的成员 --本次弹劾结束后清除',
  `actm` int(11) unsigned default '0' COMMENT '发起弹劾时间  --本次弹劾结束后清除',
  `agnst` int(11) unsigned default '0' COMMENT '反对分数 --本次弹劾结束后清除 aginst',
  `agree` int(11) unsigned default '0' COMMENT '赞成分数     --本次弹劾结束后清除',
  PRIMARY KEY  (`id`),
  KEY `id` USING BTREE (`id`),
  KEY `name` USING BTREE (`name`)
) ENGINE=MyISAM DEFAULT CHARSET=utf8 COMMENT='氏族';

